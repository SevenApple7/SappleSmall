/**shopCar.wxss**/
image{height:100%;width:100%;}
page{height: 100%;width:100%}
.container{background: #eee;height: 100%;width:100%}
.model-sl{background: #fff;margin-bottom: 25rpx;padding: 20rpx 0}
.check-wrap{width:40rpx;height:40rpx;margin-left: 30rpx;border: 1px solid #333;border-radius: 50%;display: flex;justify-content:center;align-items:center; }
.check-box{width: 50%;height: 50%;}
.model-sl{display: flex;justify-content:left;align-items:center;  }
.shop-car-img{width: 140rpx;height:140rpx;margin: 0 60rpx 0 30rpx}
.shop-car-price{display: flex;align-items:center; justify-content:space-between;padding-right: 30rpx}
.shop-car-text{flex: 1} 
.shop-car-text text{display: block}
.sct-name{font-size: 30rpx;line-height: 50rpx}
.sct-dec{font-size: 24rpx;line-height: 50rpx;color: #666}
.sct-price{font-size: 30rpx;line-height: 50rpx;color: #f6693e;}
.sct-num{font-size: 30rpx;line-height: 50rpx;}
.shop-all{position: fixed;bottom: 0;left: 0;width: 100%;height: 100rpx;background: #fff;align-items:center;display: flex;justify-content: space-between}
.check-wrap{margin-right: 20rpx}
.check-all,.price-wrap{display: flex;align-items:center;height: 100% }
.price-btn{display: flex;align-items:center;width: 80rpx;height: 100%;background: #f97127;padding: 0 60rpx;color: #fff;margin-left: 30rpx}
